  "private": true,
  "dependencies": {
    "@adobe/aio-lib-files": "^4.1.2",
    "@adobe/aio-lib-ims": "^8.1.1",
    "@adobe/aio-sdk": "^6.0.0",
    "@adobe/exc-app": "^1.4.20",
    "@adobe/react-spectrum": "^3.45.0",
    "@adobe/uix-guest": "^1.0.5",
    "@react-aria/visually-hidden": "^3.8.28",
    "core-js": "^3.46.0",
    "got": "^14.6.4",
    "node-fetch": "^3.3.2",
    "oauth-1.0a": "^2.2.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.9.6",
    "regenerator-runtime": "^0.14.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "nock": "^14.0.10",
    "wait-for-expect": "^4.0.0"
  },
  "scripts": {
    "test": "jest --passWithNoTests ./test",
    "e2e": "jest --collectCoverage=false --testRegex ./e2e",
    "lint:check": "npx eslint .",
    "lint:fix": "npm run lint:check -- --fix",
    "format:check": "npx prettier . --check",
    "format:fix": "npm run format:check -- --write",
    "code:check": "npm run format:check && npm run lint:check",
    "code:fix": "npm run format:fix && npm run lint:fix",
    "configure-events": "node -e \"require('./scripts/configure-events').main()\"",
    "configure-commerce-events": "aio console workspace download ws.json && node -e \"require('./scripts/configure-commerce-events').main('ws.json')\"; rm -f ws.json",
    "create-payment-methods": "node -e \"require('./scripts/create-payment-methods').main('payment-methods.yaml')\"",
    "create-tax-integrations": "node -e \"require('./scripts/create-tax-integrations').main('tax-integrations.yaml')\"",
    "create-shipping-carriers": "node -e \"require('./scripts/create-shipping-carriers').main('shipping-carriers.yaml')\"",
    "get-shipping-carriers": "node -e \"require('./scripts/get-shipping-carriers').main()\"",
    "sync-oauth-credentials": "node -e \"require('./scripts/sync-oauth-credentials').main()\""
  },
  "engines": {
    "node": ">=22"
  }
}
